        </LinearLayout>

        <LinearLayout
            android:id="@+id/interstitial_id_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!--            <EditText-->
            <!--                android:id="@+id/placementId3Text"-->
